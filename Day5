Do the below programs in anonymous function & IIFE

//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////1)   Print odd numbers in an array/////////////////////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////



// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here … replace the below line with your code logic 

  var arr = [1,2,3,4,5,6,7,8,9];

(function() {
  var oddArr = arr.filter(function(num) {
    return num % 2 !== 0;
  });
  console.log(oddArr);
})();

  //end-here
});
 
 
 
 
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////2)   Convert all the strings to title caps in a string array/////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////




// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here … replace the below line with your code logic 

 var arr = [1,2,3,4,5,6,7,8,9];

(function(arr) {
  var oddArr = arr.filter(function(num) {
    return num % 2 !== 0;
  });
  console.log(oddArr);
})(arr);
  //end-here
});






///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////3)   Sum of all numbers in an array/////////////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here … replace the below line with your code logic 

var strArr = ['hello', 'world', 'javascript'];

(function() {
  var titleArr = strArr.map(function(str) {
    return str.charAt(0).toUpperCase() + str.slice(1);
  });
  console.log(titleArr);
})();
  //end-here
});



